<%-include('partials/_headerOperador')%>
    <div class="container my-4 d-flex justify-content-center">
        <div class="container">
            <%if(typeof estacionamiento != 'undefined'){%>
                <div class="row my-5">
                    <div class="col">
                        <h3> Estacionamiento a cargo: </h3>
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th scope="col">ID Estacionamiento</th>
                                    <th>Ubicacion</th>
                                    <th>Horario</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        <%=estacionamiento.identificador%>
                                    </td>
                                    <td>
                                        <%=estacionamiento.ubicacion%>
                                    </td>
                                    <td>
                                        <%=estacionamiento.horaApertura%> -
                                            <%=estacionamiento.horaCierre%>
                                    </td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
                <%}%>
        </div>
    </div>



    <%-include('partials/_footerOperador')%>