<%- include ('partials/_headerAdmin')  %>
    <div class="container my-4 d-flex justify-content-center">
        <div class="row">
            <div class="row my-5">
                <label for="lista" class="my-3 fs-3 text">Lista de funcionarios registrados:</label>
                <div id="lista" class="container">
                    <%if(data){%>
                        <%for(var i=0; i<data.length;i++){%>
                            <div class="row my-3">
                                <div class="col-3">
                                    <label for="">Identificaci√≥n: <%=data[i].identificacion%></label>
                                </div>
                                <div class="col-3">
                                    <label for="">Nombre: <%=data[i].nombre%> <%=data[i].primerApellido%> <%=data[i].segundoApellido%></label>
                                </div>
                                <div class="col-3">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-4">
                                                <a href="edicionFuncionario/<%=data[i].identificacion%>" class="btn btn-primary " role="button">Editar</a>
                                            </div>
                                            <div class="col-4">
                                                <a href="eliminarFuncionario/<%=data[i].identificacion%>" class="btn btn-danger " role="button">Eliminar</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        <%}%>
                    <%}%>
                </div>
            </div>
        </div>
    </div>
<%- include ('partials/_footerAdmin')  %>